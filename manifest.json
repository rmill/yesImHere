{
    "name": "YesImHere",
    "manifest_version": 2,
    "version": "1.0 ",
    "description": "Removes the annoying 'Are you still listening?' modal",
    "icons": {
        "128": "library/img/128x128.png"
    },
    "content_scripts": [{
        "matches": [
            "http://grooveshark.com/*"
        ],
        "js": [
            "contentScript.js"
        ]
    }],
    "web_accessible_resources": ["library/jquery-2.1.1.min.js", "script.js"]
}